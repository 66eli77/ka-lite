<div class="tabbable tabs-left">
	<ul class="nav nav-tabs">
		{{#each questions}}	
		<li {{#if @first}}class="active"{{/if}}>
			<a href="#{{ anchor }}" data-toggle="tab" aria-expanded="true">Question {{ question }}</a>
		</li>
		{{/each}}
	</ul>	
	<div class="tab-content">
		{{#each questions}}
		<div {{#if @first}}class="tab-pane active col-md-10"{{else}}class="tab-pane col-md-10"{{/if}} id="{{ anchor }}">
			<table class="table table-striped table-bordered">
			<thead>
			<tr>
				<th>Attempt</th>
				<th>Action</th>
				<th>Duration</th>
				<th>Status</th>
			</tr>
			</thead>
			<tbody>
			{{#each events}}
			<tr>
				<td>{{ attempt }}</td>
				<td>{{ action }}</td>  
				<td>{{ duration }}</td>  
				<td>{{ status }}</td>
			</tr>
			{{/each}}
			</tbody>
			</table>
		</div>
		{{/each}}
	</div>
</div>

